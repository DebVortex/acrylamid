
<entry>
    <title>{{ title }}</title>
    <link rel="alternate" type="text/html" href="{{link}}" />
    <id>{{ link }}</id>
    <updated>{{date}}</updated>
    <author>
        <name>{{ author }}</name>
        <uri>{{ website }}</uri>
        <email>{{ email }}</email>
    </author>
    <content type="html">{{ content }}</content>
</entry>
